    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="Features/Common/Resources/Theme/ColorDictionary.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Converters/Converters.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/CheckBoxStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/RadioButtonStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/TextStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/ToggleSwitchStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/ContainerStyles.xaml"/>
                <ResourceDictionary Source="Features/Common/Resources/Styles/DialogStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
